{
	"name": "@agnos-ui/svelte-demo",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "wireit",
		"dev:coverage": "wireit",
		"build": "wireit",
		"build:coverage": "wireit",
		"check": "wireit"
	},
	"wireit": {
		"sveltekit": {
			"command": "svelte-kit sync",
			"dependencies": [
				"../preprocess:build"
			],
			"files": [
				"src/**",
				"svelte.config.js"
			],
			"output": [
				".svelte-kit/**",
				"!.svelte-kit/{generated,output}/**"
			]
		},
		"dev": {
			"command": "vite",
			"service": true,
			"dependencies": [
				"sveltekit",
				"../bootstrap:generate:exports",
				"../bootstrap:generate:component-types",
				"../headless:generate:exports"
			]
		},
		"dev:coverage": {
			"command": "vite -c vite.coverage.config.ts",
			"service": true,
			"dependencies": [
				"../../code-coverage:build",
				"sveltekit",
				"../bootstrap:generate:exports",
				"../bootstrap:generate:component-types",
				"../headless:generate:exports"
			]
		},
		"build": {
			"command": "vite build",
			"dependencies": [
				"sveltekit",
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"vite.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"build:coverage": {
			"command": "vite build -c vite.coverage.config.ts",
			"dependencies": [
				"../../code-coverage:build",
				"sveltekit",
				"../bootstrap:generate:exports",
				"../headless:generate:exports"
			],
			"files": [
				"src/**",
				"vite.config.ts",
				"vite.coverage.config.ts"
			],
			"output": [
				"dist/**"
			]
		},
		"check": {
			"command": "svelte-check --fail-on-warnings --output human-verbose --tsconfig tsconfig.check.json",
			"dependencies": [
				"../bootstrap:build"
			],
			"files": [
				"src/**",
				"tsconfig.check.json",
				"../tsconfig.json",
				"../../tsconfig.json"
			],
			"output": []
		}
	},
	"dependencies": {
		"@agnos-ui/core-bootstrap": "0.0.0",
		"@agnos-ui/svelte-bootstrap": "0.0.0",
		"@amadeus-it-group/tansu": "^1.0.0",
		"@floating-ui/dom": "^1.6.12"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.8.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.14",
		"postcss": "^8.4.49",
		"prettier-plugin-svelte": "^3.2.8",
		"svelte": "^5.2.3",
		"svelte-check": "^4.0.9",
		"tailwindcss": "^3.4.15"
	}
}
